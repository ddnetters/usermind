{
  "name": "@usermind/core",
  "version": "0.0.1",
  "description": "Flow DSL, guidance engine, and evidence capture for Usermind",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
