  },
  "keywords": [
    "usermind",
    "flow-dsl",
    "mcp",
    "evidence",
    "agent-guidance"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "yaml": "^2.8.2"
  }
}
